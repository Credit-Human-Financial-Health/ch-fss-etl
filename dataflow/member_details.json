{
	"name": "member_details",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "member_detailscsv",
						"type": "DatasetReference"
					},
					"name": "source"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "member_details_sink",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "derivedColumn2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          add_date as string,",
				"          PWC_Key as short,",
				"          ORDINAL as boolean,",
				"          TYPE as short,",
				"          SUBTYPE as boolean,",
				"          MAILOVERRIDE as boolean,",
				"          SSNOVERRIDE as boolean,",
				"          SSNCERTIFICATION as boolean,",
				"          SSNTYPE as boolean,",
				"          EFFECTIVEDATE as string,",
				"          EXPIRATIONDATE as string,",
				"          FirstName as string,",
				"          MiddleName as string,",
				"          LastName as string,",
				"          SUFFIX as string,",
				"          EXTRAADDRESS as string,",
				"          STREET as string,",
				"          CITY as string,",
				"          STATE as string,",
				"          ZIPCODE as string,",
				"          LASTFMDATE as string,",
				"          CARRIERROUTE as string,",
				"          ADDRESSTYPE as short,",
				"          DEATHDATE as string,",
				"          ECOACODE as boolean,",
				"          COUNTRY as string,",
				"          COUNTRYCODE as string,",
				"          RECORDCHANGEDATE as string,",
				"          MBRSTATUS as short,",
				"          MBRSTATUSFMDATE as string,",
				"          MBRCREATEDATE as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     partitionBy('hash', 1)) ~> source",
				"source derive(name = replace(FirstName, '\\u0000', ''),",
				"          {last_name     } = replace(LastName, '\\u0000', '')\r",
				",",
				"          address = replace(STREET, '\\u0000', ''),",
				"          city = replace(CITY, '\\u0000', ''),",
				"          state = replace(STATE, '\\u0000', '')\r",
				",",
				"          zip_code = replace(ZIPCODE, '\\u0000', '')\r",
				",",
				"          status = toString(MBRSTATUS),",
				"          start_date = toDate(trim(replace(MBRCREATEDATE, ' +00', '')), 'yyyy-MM-dd'),",
				"          member_id = toInteger(PWC_Key, '[^0-9]', '')) ~> derivedColumn1",
				"derivedColumn1 derive(first_name = iif(isNull(name) || length(trim(name)) == 0, 'Unknown', replace(name, '\\u0000', ''))) ~> derivedColumn2",
				"derivedColumn2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          member_id as integer,",
				"          name as string,",
				"          last_name as string,",
				"          date_of_birth as date,",
				"          ssn as string,",
				"          address as string,",
				"          city as string,",
				"          state as string,",
				"          zip_code as string,",
				"          phone as string,",
				"          email as string,",
				"          branch as string,",
				"          status as string,",
				"          start_date as date",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     mapColumn(",
				"          member_id,",
				"          name = first_name,",
				"          last_name = {last_name     },",
				"          address,",
				"          city = derivedColumn1@city,",
				"          state = derivedColumn1@state,",
				"          zip_code,",
				"          status,",
				"          start_date",
				"     ),",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}