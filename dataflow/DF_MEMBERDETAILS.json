{
	"name": "DF_MEMBERDETAILS",
	"properties": {
		"folder": {
			"name": "FSS_DATAFLOW"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "MEMBERDETAILSSOURCE"
				}
			],
			"sinks": [
				{
					"name": "MEMBERDETAILSTABLE",
					"rejectedDataLinkedService": {
						"referenceName": "AzureBlobStorage1",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [
				{
					"name": "COLUMNSDERIVED"
				}
			],
			"scriptLines": [
				"source(output(",
				"          add_date as string,",
				"          PWC_Key as integer,",
				"          ORDINAL as string,",
				"          TYPE as string,",
				"          SUBTYPE as string,",
				"          MAILOVERRIDE as string,",
				"          SSNOVERRIDE as string,",
				"          SSNCERTIFICATION as string,",
				"          SSNTYPE as string,",
				"          EFFECTIVEDATE as string,",
				"          EXPIRATIONDATE as string,",
				"          FirstName as string,",
				"          MiddleName as string,",
				"          LastName as string,",
				"          SUFFIX as string,",
				"          EXTRAADDRESS as string,",
				"          STREET as string,",
				"          CITY as string,",
				"          STATE as string,",
				"          ZIPCODE as string,",
				"          LASTFMDATE as string,",
				"          CARRIERROUTE as string,",
				"          ADDRESSTYPE as string,",
				"          DEATHDATE as string,",
				"          ECOACODE as string,",
				"          COUNTRY as string,",
				"          COUNTRYCODE as string,",
				"          RECORDCHANGEDATE as string,",
				"          MBRSTATUS as string,",
				"          MBRSTATUSFMDATE as string,",
				"          MBRCREATEDATE as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionBy('hash', 1)) ~> MEMBERDETAILSSOURCE",
				"MEMBERDETAILSSOURCE derive(name = replace(FirstName, char(0), ''),",
				"          {e-mail} = LastName,",
				"          state = substring(rtrim(STATE), 1, 2)) ~> COLUMNSDERIVED",
				"COLUMNSDERIVED sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          member_id = PWC_Key,",
				"          name,",
				"          last_name = LastName,",
				"          address = STREET,",
				"          city = CITY,",
				"          state = COLUMNSDERIVED@state,",
				"          zip_code = ZIPCODE,",
				"          start_date = MBRCREATEDATE,",
				"          email = {e-mail}",
				"     ),",
				"     format: 'table') ~> MEMBERDETAILSTABLE"
			]
		}
	}
}